{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "never"]
    },
    "globals": {
        "d3": "writable",
        "topojson": "writable",
        "versor": "writable",
        "Choices": "writable",
        "firebase": "writable",
        "Swal": "writable"
    }
}
